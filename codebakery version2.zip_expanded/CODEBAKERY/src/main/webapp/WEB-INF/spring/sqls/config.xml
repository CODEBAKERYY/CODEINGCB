  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="com.mvc.cb.model.dto.UserDto" alias="UserDto" />
		<typeAlias type="com.mvc.cb.model.dto.QuizDto" alias="quizDto" />
		<typeAlias type="com.mvc.cb.model.dto.MentorDto" alias="MentorDto" />
